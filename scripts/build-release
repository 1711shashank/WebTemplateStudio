dotnet publish ../src/CoreTemplateStudio/code/src/CoreTemplateStudio/CoreTemplateStudio.Cli/CoreTemplateStudio.Cli.csproj -c release -r osx-x64 --self-contained true -p:PublishSingleFile=true -o ../src/extension/src/corets-cli/darwin/