{
  "regexs" : [
    {
      "name":"nameStartLetter",
      "pattern" : "^[A-Za-z]"
    },
    {
      "name":"nameContainLettersNumbersDashes",
      "pattern" : "^((?!\\d)[a-zA-Z0-9_-]+)$"
    }
  ],
  "reservedNames" : ["reserve1"],
  "validateEmptyNames": true,
  "validateExistingNames": true
}