{
	"extends": ["tslint:latest", "tslint-react"],
	"linterOptions": {
		"exclude": [
			"src/serviceWorker.ts"
		]
	},
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": false,
		"jsx-wrap-multiline": true
	},
	"defaultSeverity": "warning"
}