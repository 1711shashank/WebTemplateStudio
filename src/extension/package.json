{
  "name": "extension",
  "displayName": "Web Template Studio",
  "description": "An extension that helps developers get started quickly when building web apps by generating web templates which encourage best code practices.",
  "version": "0.0.1",
  "publisher": "Microsoft",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:webTemplateStudioExtension.wizardLaunch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webTemplateStudioExtension.wizardLaunch",
        "title": "%webTemplateStudioExtension.commands.wizardLaunch%",
        "category": "%webTemplateStudioExtension.commands.wts%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "createVsixPackage": "rm -rf ../../dist/wts.vsix && vsce package --out ../../dist/wts.vsix"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "extensionDependencies": [
    "ms-azuretools.vscode-cosmosdb"
  ],
  "dependencies": {
    "azure-arm-resource": "^7.3.0",
    "global": "^4.3.2",
    "ms-rest": "^2.5.0",
    "ms-rest-azure": "^2.6.0",
    "vsce": "^1.57.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/WebTemplateStudio"
  }
}
