{
  "name": "webtemplatestudio",
  "displayName": "Web Template Studio",
  "description": "A VS Code extension developer tool to accelerate web application scaffolding.",
  "license": "SEE LICENSE IN LICENSE.md",
  "icon": "images/Market_AzureAccount_128x.png",
  "repository": {
    "url": "https://github.com/microsoft/vscode-webTemplateStudio.git"
  },
  "homepage": "https://github.com/Microsoft/WebTemplateStudio/blob/dev/README.md",
  "galleryBanner": {
    "color": "#0072c6",
    "theme": "dark"
  },
  "version": "0.8.0",
  "aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
  "publisher": "ms-vscode",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Azure"
  ],
  "keywords": [
    "azure"
  ],
  "activationEvents": [
    "onCommand:webTemplateStudio.login",
    "onCommand:webTemplateStudio.loginWithDeviceCode",
    "onCommand:webTemplateStudio.logout",
    "onCommand:webTemplateStudio.loginToCloud",
    "onCommand:webTemplateStudio.selectSubscriptions",
    "onCommand:webTemplateStudio.createAccount",
    "onCommand:webTemplateStudio.launch"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "webTemplateStudio.login",
        "title": "%webTemplateStudio.commands.login%",
        "category": "%webTemplateStudio.commands.wts%"
      },
      {
        "command": "webTemplateStudio.logout",
        "title": "%webTemplateStudio.commands.logout%",
        "category": "%webTemplateStudio.commands.wts%"
      },
      {
        "command": "webTemplateStudio.loginToCloud",
        "title": "%webTemplateStudio.commands.loginToCloud%",
        "category": "%webTemplateStudio.commands.wts%"
      },
      {
        "command": "webTemplateStudio.selectSubscriptions",
        "title": "%webTemplateStudio.commands.selectSubscriptions%",
        "category": "%webTemplateStudio.commands.wts%"
      },
      {
        "command": "webTemplateStudio.createAccount",
        "title": "%webTemplateStudio.commands.createAccount%",
        "category": "%webTemplateStudio.commands.wts%"
      },
      {
        "command": "webTemplateStudio.launch",
        "title": "%webTemplateStudio.commands.launch%",
        "category": "%webTemplateStudio.commands.wts%"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Azure configuration",
      "properties": {
        "azure.resourceFilter": {
          "type": "array",
          "default": null,
          "description": "The resource filter, each element is a tenant id and a subscription id separated by a slash."
        },
        "azure.showSignedInEmail": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show the email address (e.g., in the status bar) of the signed in account."
        },
        "azure.tenant": {
          "type": "string",
          "default": "",
          "description": "A specific tenant to sign in to. The default is to sign in to the common tenant and use all known tenants."
        },
        "azure.cloud": {
          "type": "string",
          "enum": [
            "Azure",
            "AzureChina",
            "AzureGermanCloud",
            "AzureUSGovernment"
          ],
          "enumDescriptions": [
            "Azure",
            "Azure China",
            "Azure Germany",
            "Azure US Government"
          ],
          "default": "Azure",
          "description": "The current Azure Cloud to connect to."
        },
        "azure.ppe": {
          "type": "object",
          "properties": {
            "portalUrl": {
              "type": "string"
            },
            "publishingProfileUrl": {
              "type": "string"
            },
            "managementEndpointUrl": {
              "type": "string"
            },
            "resourceManagerEndpointUrl": {
              "type": "string"
            },
            "sqlManagementEndpointUrl": {
              "type": "string"
            },
            "sqlServerHostnameSuffix": {
              "type": "string"
            },
            "galleryEndpointUrl": {
              "type": "string"
            },
            "activeDirectoryEndpointUrl": {
              "type": "string"
            },
            "activeDirectoryResourceId": {
              "type": "string"
            },
            "activeDirectoryGraphResourceId": {
              "type": "string"
            },
            "batchResourceId": {
              "type": "string"
            },
            "activeDirectoryGraphApiVersion": {
              "type": "string"
            },
            "storageEndpointSuffix": {
              "type": "string"
            },
            "keyVaultDnsSuffix": {
              "type": "string"
            },
            "azureDataLakeStoreFileSystemEndpointSuffix": {
              "type": "string"
            },
            "azureDataLakeAnalyticsCatalogAndJobEndpointSuffix": {
              "type": "string"
            },
            "validateAuthority": {
              "type": "boolean"
            }
          },
          "default": null,
          "description": "Development setting: The PPE environment for testing."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "rimraf dist && webpack --mode production",
    "webpack": "rimraf dist && webpack --mode development",
    "webpack-dev": "rimraf dist && webpack --mode development --watch",
    "compile": "tsc -p ./",
    "which": "which tsc",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/http-proxy-agent": "2.0.1",
    "@types/keytar": "4.0.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "8.0.26",
    "@types/request": "2.48.1",
    "@types/request-promise": "4.1.42",
    "@types/semver": "5.5.0",
    "@types/ws": "6.0.1",
    "rimraf": "2.6.2",
    "ts-loader": "5.3.0",
    "typescript": "3.1.6",
    "vscode": "^1.1.22",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "adal-node": "0.1.28",
    "azure-arm-resource": "^7.0.1",
    "form-data": "2.3.3",
    "http-proxy-agent": "2.1.0",
    "https-proxy-agent": "2.2.1",
    "ms-rest": "^2.3.8",
    "ms-rest-azure": "^2.5.9",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "semver": "5.6.0",
    "vscode-extension-telemetry": "^0.1.0",
    "vscode-nls": "^4.0.0",
    "ws": "6.1.0"
  },
  "extensionDependencies": [
    "ms-azuretools.vscode-cosmosdb"
  ]
}